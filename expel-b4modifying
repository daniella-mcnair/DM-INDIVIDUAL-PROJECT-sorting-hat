const expelStudent = (event) => {
  if(event.target.id.includes("delete")){
    const[, id] = event.target.id.split("--")
    const index = students.findIndex(obj => obj.id === id);
    students.splice(index,1)
    sortStudents(students);
  }


}




const expelStudent = (event) => {
  if(event.target.id.includes("delete")){
    const[, id] = event.target.id.split("--")
    const index = students.findIndex(obj => obj.id === id);
    students.splice(index,1)
    sortStudents(students);
  }

  const removedStudent = students.splice(index, 1)[0];
  expelledStudents.push(removedStudent)
}


const renderToTeamMoldy = (array) => {
  expelledStudents.forEach((expelledStudent) => {
  let domstring = ""
  domstring =+
    `<div class="card" id = "HouseCard" style="width: 18rem;">
    <div class="card-header">
    </div>
    <div class="card-body">
    <img src="" class="img" alt="">
      <p id="studentHouse">Unfortunately, ${expelledStudent.name} has gone over to the dark side :(</p>
    </div>
  </div>`
  })
  teamMoldyArray.innerHTML = domstring
}
